<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefHeight="400.0" prefWidth="600.0" maxWidth="Infinity" maxHeight="Infinity"
      fx:controller="dev.donkz.pendragon.controller.VariantListController">
    <children>
        <Label text="Your Variants">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
        <TilePane fx:id="tilePane"
              maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0"
              prefWidth="200.0" VBox.vgrow="ALWAYS" hgap="10" vgap="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </TilePane>
    </children>
</VBox>
