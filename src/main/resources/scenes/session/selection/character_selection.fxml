<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="Infinity" maxWidth="Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="dev.donkz.pendragon.controller.CharacterSessionController">
    <children>
        <Label text="Your Characters">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
        <TilePane fx:id="tilePane" hgap="10" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  prefHeight="200.0" prefWidth="200.0" vgap="10" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </TilePane>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" text="Cancel" onAction="#onCancel"/>
                <Button fx:id="btnCreate" mnemonicParsing="false" onAction="#onCreate" text="Create"/>
            </buttons>
        </ButtonBar>
    </children>
</VBox>
